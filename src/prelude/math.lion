operator INFIX 8 "**" (\x y = pow x y)

operator INFIX 7 "*" (\x y = mul x y)
operator INFIX 7 "/" (\x y = div x y)
operator INFIX 7 "%" (\x y = mod x y)

operator INFIX 6 "+" (\x y = add x y)
operator INFIX 6 "-" (\x y = sub x y)

operator INFIX 4 "==" (\x y = eq x y)
operator INFIX 4 "!=" (\x y = ne x y)
