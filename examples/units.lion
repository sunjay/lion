#[unit] //TODO: SI prefixes (lowercase)
fn(x '_) -> 'L { x }

#[converter]
fn(x 'm^3) -> 'L { x * 1000 }
#[converter]
fn(x 'L) -> 'm^3 { x / 1000 }

