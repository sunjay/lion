// Angles

#[unit]
fn(x '_) -> 'deg { x }
#[unit]
fn(x '_) -> 'rad { x }

#[converter]
fn(x 'deg) -> 'rad { x * (pi 'rad) / (180 'deg) }
#[converter]
fn(x 'rad) -> 'deg { x * (180 'deg) / (pi 'rad) }

// Length

#[unit] //TODO: SI prefixes
fn(x '_) -> 'm { x }
#[unit]
fn(x '_) -> 'in { x }

#[converter]
fn(x 'in) -> 'm { x * 0.0254 }
#[converter]
fn(x 'm) -> 'in { x / 0.0254 }

