#[unit]
fn(x '_) -> 'deg { x }
#[unit]
fn(x '_) -> 'rad { x }

#[converter]
fn(x 'deg) -> 'rad { x * (pi 'rad) / (180 'deg) }
#[converter]
fn(x 'rad) -> 'deg { x * (180 'deg) / (pi 'rad) }

#[unit]
#[prefix(SI)]
fn(x '_) -> 'm { x }

#[unit]
#[prefix(SI, transform=[lowercase])]
fn(x '_) -> 'L { x }

#[converter]
fn(x 'm^3) -> 'L { x * 1000 }
#[converter]
fn(x 'L) -> 'm^3 { x / 1000 }
